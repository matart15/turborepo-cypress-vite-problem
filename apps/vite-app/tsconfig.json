{
  "display": "Default",
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "incremental": false,
    "inlineSources": false,
    "preserveWatchOutput": true,
    "typeRoots": [
      "../../node_modules/@types",
      "types"
    ],
    "skipLibCheck": true,
    "esModuleInterop": false,
    "jsx": "react-jsx",
    "types": [
      "vite/client",
      "node",
      "jest"
    ],
    "baseUrl": "."
  },
  "include": [
    "src",
    "types"
  ],
  "exclude": [
    "node_modules",
    "vite.config.ts"
  ],
}